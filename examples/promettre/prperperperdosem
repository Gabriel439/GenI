%
% "Jean promet a Marie de persuader Paul de persuader Pierre de
% persuader Olivier de donner
% un livre a Claire" 

semantics : [ e1:promise(a b e2)
	      a:jean(a)
	      b:marie(b)
	      e2:convince(a d e3)
	      d:paul(d)
	      e3:convince(d e e4)
	      e:pierre(e)
	      e4:convince(e o e5)
	      o:olivier(o)
	      e5:give(o l f)
	      indef(l)
	      l:book(l)
	      f:claire(f)
	     ]


