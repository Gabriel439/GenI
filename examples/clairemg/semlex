
%%--------------------------------------------------------------------- 
%% Verbs
%% --------------------------------------------------------------------- 

est est []
semantics:[]

regarde v [evt:E arg1:X arg2:Y]
semantics:[lookAt(E) agent(E X) patient(E Y)]

%%%
