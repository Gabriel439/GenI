semantics:[john(X)]

marie n [idx:X]
semantics:[mary(X)]

se se [idx:X]
semantics:[]

c ce  []
semantics:[]

il il [idx:X]
semantics:[]

la laCl [idx:X]
semantics:[]

le leCl [idx:X]
semantics:[]

qui qui [idx:X] 
semantics:[]

qui qui [idx:X] 
semantics:[]

que que [idx:X] 
semantics:[]

que que [idx:X] 
semantics:[]

