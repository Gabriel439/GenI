% there should be exactly one way to produce the answer here
% (as opposed to two) - the idea is that GenI enforces the
% condition that only trees with empty subst nodes list may
% subst up
abc semantics:[a() b() t1()]
sub semantics:[b() t1()]

% adjunction 
adj semantics:[a() xg()]
adj_multi semantics:[a() xg() xg2()]

% kidsToParents rule
k2p         semantics:[t1() d()]
k2p_trivial semantics:[k2p()]

% things which should not work!
k2p_conflict semantics:[t1() t2() db()]
adj_failure  semantics:[a() xb()] % top/bot conflict

tb_success   semantics:[eg()]
tb_failure1  semantics:[eb1()]
tb_failure2  semantics:[eb2() t1() t2()]

