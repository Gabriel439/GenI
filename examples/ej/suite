v_runs 
semantics: [ name(s1 a vincent)
             run(e a) ]

v_loves_m 
semantics: [ name(s1 a vincent)
             love(e a ex)
             name(s2 ex mia)]

v_loves_é
semantics: [ name(s1 a vincent)
             love(e a ex)
             name(s2 ex émilie)]

v_loves_m_f
semantics : [name(s1 a vincent)
             love(e a ex)
             fast(s3 e)
             name(s2 ex mia)]

big_tall_man_run
semantics : [def(a)
             man(s1 a)
             run(e a)
             big(s3 a)
             tall(s3 a)
            ]

man_love_m
semantics: [man(s1 a) 
            love(e a ex)
            name(s2 ex mia)]

man_remove_rabbit
semantics : [man(s1 a) 
             def(a)
             remove(e a ex h)
             def(ex)
	     rabbit(s2 ex)
	     hat(s3 h)
             def(h)]

man_remove_white_rabbit_fast
semantics : [man(s1 a)
             remove(e a ex h)
	     rabbit(s2 ex)
	     white(s6 ex)
	     fast (s5 e)
	     hat(s3 h)]

man_remove_rabbit_fast
semantics : [man(s1 a) 
             remove(e a ex h)
             def(a)
	     fast(s4 e)
	     rabbit(s2 ex)
             def(ex)
	     hat(s3 h)
             def(h)]

man_remove_rabbit_fast_def
semantics : [man(s1 a)
	     def(a) 
             remove(e a ex h)
	     fast(s4 e)
	     rabbit(s2 ex)
	     def(ex)
	     white(s4 ex)
	     def(h)
	     hat(s3 h)]

