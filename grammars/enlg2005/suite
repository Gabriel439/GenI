%             organiser(org m c) ]

AR
  semantics: [ directeur(m)    def(m)     de(m o) 
             bureau(o)       dem(o)                      
             consultant(c)   indef(c) nouveau(c)                 
             de(c g)         allemagne(g)             
             auditioner(int m c) 
%             jean(j) promettre(pro m int j) 
           ]
restrictors:[ cleft:c ]

