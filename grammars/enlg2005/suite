             seminaire(c) indef(c) dequipe(c)  nouveau(c) 
	     hebdomadaire(c)
             organiser(org m c) ]

BR
semantics: [ directeur(m) poss(m us)
             seminaire(c) indef(c) special(c) hebdomadaire(c)
             nouveau(c) dequipe(c)
             organiser(org m c) ]
restrictors: [cleft:c]

